<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite All" verbose="1">
    <test name="Regression Test with firefox">
        <parameter name="browserTest" value="firefox"></parameter>
        <classes>
            <class name="automation.testsuite.ProjectEvents_EventType_Team2"/>
            <class name="automation.testsuite.ProjectEvents_AddEvent_Team2"></class>
            <class name="automation.testsuite.ProjectEvents_DisplayByMonthWeekDay_Team2"></class>
        </classes>
    </test>

    <test name="Regression Test with edge">
        <parameter name="browserTest" value="edge"></parameter>
        <classes>
            <class name="automation.testsuite.ProjectEvents_EventType_Team2"/>
            <class name="automation.testsuite.ProjectEvents_AddEvent_Team2"></class>
            <class name="automation.testsuite.ProjectEvents_DisplayByMonthWeekDay_Team2"></class>
        </classes>
    </test>

    <test name="Regression Test with chrome">
        <parameter name="browserTest" value="chrome"></parameter>
        <classes>
            <class name="automation.testsuite.ProjectEvents_EventType_Team2"/>
            <class name="automation.testsuite.ProjectEvents_AddEvent_Team2"></class>
            <class name="automation.testsuite.ProjectEvents_DisplayByMonthWeekDay_Team2"></class>
        </classes>
    </test>

</suite>